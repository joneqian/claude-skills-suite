  "selectors": {
    "main_content": "#content, .prose, [class*='content']",
    "title": "h1",
    "code_blocks": "pre code"
  },
  "url_patterns": {
    "include": ["/oss/python/langchain/", "/oss/python/integrations/"],
    "exclude": [
      "/oss/javascript/",
      "/oss/python/langgraph/",
      "/oss/python/deep-agents/",
      "/langsmith/",
      "/changelog",
      "/contribute",
      "/reference"
    ]
  },
  "categories": {
    "getting_started": [
      "overview",
      "install",
      "quickstart",
      "concepts",
      "philosophy"
    ],
    "chains": ["chain", "lcel", "runnable", "sequence"],
    "agents": ["agent", "tool", "toolkit", "react"],
    "memory": ["memory", "history", "buffer", "conversation"],
    "retrieval": [
      "retriever",
      "vectorstore",
      "embedding",
      "document",
      "loader",
      "splitter"
    ],
    "llm": ["llm", "chat", "model", "prompt", "template"],
    "integrations": [
      "openai",
      "anthropic",
      "google",
      "azure",
      "bedrock",
      "ollama"
    ],
    "advanced": ["callbacks", "streaming", "caching", "tracing"]
  },
  "rate_limit": 10,
  "max_pages": 300,
  "use_playwright": true,
  "skip_llms_txt": true,
  "playwright_config": {
    "wait_until": "domcontentloaded",
    "timeout": 120000,
    "extra_wait": 3000,
    "max_retries": 5
  }
}
